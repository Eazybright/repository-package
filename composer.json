{
    "name": "eazybright/repository-package",
    "description": "A package for managing repository design pattern",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "package",
        "laravel",
        "repository",
        "eazybright"
    ],
    "authors": [
      {
        "name": "Kolawole Ezekiel",
        "email": "doneazy911@gmail.com"
      }
    ],
    "require": {
      "php": "^7.2",
      "illuminate/support": "^8.*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
          "Eazybright\\RepositoryPackage\\": "src/"
        }
      },
      "extra": {
        "laravel": {
            "providers": [
                "Eazybright\\RepositoryPackage\\RepositoryPackageServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eazybright\\RepositoryPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
      "test": "vendor/bin/phpunit",
      "test-f": "vendor/bin/phpunit --filter"
    },
    "minimum-stability": "dev"
}
